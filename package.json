    "secrets-encrypt-debug": "tsn ./src/bin/secrets-encrypt.ts",
    "secrets-decrypt-debug": "tsn ./src/bin/secrets-decrypt.ts",
    "kpy-debug": "tsn ./src/bin/kpy.ts node_modules dist",
